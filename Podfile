# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CASTA' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CASTA
  pod 'Alamofire', '~> 5.2'
  pod 'SnapKit', '~> 5.0.0'
  
  
  target 'CASTATests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire', '~> 5.2'
    
  end

  target 'CASTAUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Alamofire', '~> 5.2'
    pod 'SnapKit', '~> 5.0.0'
  end

end
